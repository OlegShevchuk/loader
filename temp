      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

      (function(){
        window.gaHandler = {
          trackers: {},

          stringToArray: function(str, delimiter)
          {
            delimiter = delimiter || ';';
            var re = new RegExp(delimiter + '+$');
            str = str.replace(re, '');
            return str.split(delimiter);
          },

          track: function(data)
          {
            if(typeof(data) == 'string')
              data = this.stringToArray(data);

            if($.isArray(data) && data.length >= 2)
            {
              var tName = this.getTrackerName(data[0]);
              if(tName && !this.trackers[tName])
              {
                data[0] = data[0].substr(tName.length + 2);
              }

              ga.apply(null, data);
            }
          },

          trackNode: function(node, attr)
          {
            attr = attr || 'data-ga-event';
            var data = $(node).attr(attr);
            if(data)
              this.track(data);
          },

          getTrackerName: function(command)
          {
            var i = command ? command.indexOf('.') : -1;
            if(i <= 0)
              return;

            return command.substr(0, i);
          }
        };

        $('body').on('click sendstats', 'a.ga_track_events', function(event){
          gaHandler.trackNode(this);
        });
     })();


