<script>
  $(function(){
    if (!window.url || !(_sf.query.hti == 1 || _sf.hash.hti == 1)){
      return;
    }

    if (url.search(/^https?:\/\/([a-z]+\.)?youtube\.com\/(#!?\/)?watch\?.*v=/i) == -1) {
      return;
    }

    var cn = 'hti', cv = cookie.get(cn), max = 5,
        ec = 'popaptest1';

    if(cv === '0')
      return;

    if (!cv) {
      cv = max;
    }
    else {
      cv = parseInt(cv);
      if (cv <= 0 || isNaN(cv)) {
        setCookie(0);
        return;
      }
      else if (cv > max) {
        cv = max;
      }
    }

    $('#js-info-msg1 a.action').on('click', function(event){
      event.preventDefault();
      setCookie(0);
      $.fancybox.close();
      ga('send', 'event', ec, 'click', 'button');
    });

    setTimeout(function(){
      $.fancybox({
        href: "#js-info-msg1",
        type: 'inline',
        beforeShow: function() {
          setCookie(cv - 1);

          if (window.ga) {
            ga('send', 'event', ec, 'show');
          }
        }
      });
    }, 2000);

    function setCookie (val) {
      cookie.set(cn, val.toString(), 90, _sf.cookieDomain);
    }
  });
</script>
