<script>
  var OA_zones;

  (function(){
    $('.a1d').each(function(){
      var zoneID = $(this).data('openx').zoneID;
      if(!zoneID)
        return;

      if(!this.id)
        this.id = 'ox_zoneid_' + zoneID;

      if(!OA_zones)
        OA_zones = {};

      OA_zones[this.id] = zoneID;
    });

    if(!OA_zones)
      return;

    var src = 'http://78.138.126.253/www/a9s/spcjs.php?id=1';

    if(document.cookie && document.cookie.indexOf('browser_ext=') > -1)
      src += '&helper_installed=1';

    document.write('<script src="' + src + '"><\/script>');

    var counter = 0;

    function displayBannerAds()
    {
      if(window.OA_output)
      {
        for(var zone_div_id in OA_zones)
        {
          var zoneid = zone_div_id;

          if(!OA_output[zoneid])
            zoneid = OA_zones[zone_div_id];

          if(OA_output[zoneid])
          {
            var flashCode, oDIV = document.getElementById( zone_div_id );
            if (oDIV)
            {
              // if it's a flash banner..
              if(OA_output[zoneid].indexOf("ox_swf.write") != -1)
              {
                // extract javascript code
                var pre_code_wrap = "<script type='text/javascript'><!--// <![CDATA[",
                  post_code_wrap = "// ]]> -->";

                flashCode = OA_output[zoneid].substr(OA_output[zoneid].indexOf(pre_code_wrap) + pre_code_wrap.length);
                flashCode = flashCode.substr(0, flashCode.indexOf(post_code_wrap));

                // replace destination for the SWFObject
                flashCode = flashCode.replace(/ox\_swf\.write\(\'(.*)'\)/, "ox_swf.write('" + oDIV.id + "')");

                // insert SWFObject
                if( flashCode.indexOf("ox_swf.write") != -1 )
                {
                  eval(flashCode);
                  $(oDIV).show();
                }
                // else: the code was not as expected; don't show it
              }
              else
              {
                // normal image banner; just set the contents of the DIV
                $(oDIV).html(OA_output[zoneid]).addClass('loaded');
                $(oDIV).show();
              }
            }
          }
        }//else: no banners on this page
      }
      else
      {
        // not ready, let's wait a bit
        if (counter > 30)
        {
          return; // we waited too long; abort
        }
        setTimeout( displayBannerAds, 1000);
        counter++;
      }
    }

    setTimeout(displayBannerAds, 1000);
  })();
</script>
<script>
$(function(){
  var widgets = ['fb'], width = 370, height = 505, index = 0,
    $box = $('#social_group_widget'), lang = 'ru';

  if(lang == 'ru' || lang == 'uk')
  {
    //widgets.push('vk');
    widgets = ['vk'];
  }

  if($box.width() < width)
  {
    width = $box.width();
  }

  if(widgets.length > 0)
  {
    index = Math.floor(Math.random() * widgets.length);
  }

  if(isCompactView())
  {
    if($('.share42_h_init')[0])
      return;

    var $shareBtns = $('<div class="share42_h_init share42_h24"/>').attr({
      'data-path': '/img/socials/share42/',
      'data-url': $('head link[rel="canonical"]').attr('href'),
      'data-image': $('head link[rel="image_src"]').attr('href')
    }).appendTo($box);

    var el = 'script', count = 0;
    $(el).attr('src', '/js/libs/share42_horz.js').appendTo('body');

    var func = function() {
      count++;

      if(window.share42Data) {
        if(lang == 'ru') {
          share42Data.shareStr = 'Поделиться в';
          share42Data.order = ['vk', 'fb', 'twitter', 'googlePlus'];
        }
        share42Data.icons = 'icons_h24.png';
        share42Data.size = 24;
        share42HorzWithCount(jQuery,'div.share42_h_init', share42Data);

        return;
      }

      if(count > 30)
        return;

      setTimeout(func, 1000);
    };

    func();
    return;
  }

  switch(widgets[index])
  {
    case 'fb':
      createFbWidget();
      break;

    case 'vk':
      createVkWidget();
      break;
  }

  function isCompactView()
  {
    if($(document).width() <= 720)
    {
        var ua = navigator.userAgent || navigator.vendor;
        var uaRegExp = /android|avantgo|bada\/|blackberry|blazer|compal|elaine|fennec|hiptop|hpwos|iemobile|ip(hone|od|ad)|iris|j2me|kindle|lge |maemo|meego|midp|mmp|mobile|netfront|opera m(ob|in)i|palm( os)?|phone|p(ixi|re)\/|plucker|pocket|psp|series(4|6)0|symbian|treo|ucweb|uc ?browser|up\.(browser|link)|vodafone|wap|webos|windows (ce|phone)|xda|xiino/i;
        if(ua && uaRegExp.test(ua))
          return true;
    }

    return false;
  }

  function createFbWidget()
  {
    var widgetUrl = '//www.facebook.com/plugins/likebox.php?',
        groupUrl = 'https://www.facebook.com/SaveFromNetEn';

    if(lang == 'ru')
      groupUrl = 'https://www.facebook.com/pages/Savefromnet-ru/419272651512055';

    var query = {
      href: groupUrl,
      width: width,
      height: height,
      colorscheme: 'light',
      show_faces: 'false',
      header: 'false',
      stream: 'true',
      show_border: 'false'
    };

    if(isCompactView()) {
      height = 240;
      query.height = height;
      query.header = 'true';
      query.show_faces = 'true';
      query.stream = 'false';
    }

    $box.css('margin-top', '6px').
      css({
        overflow: 'hidden',
        height: height + 'px'
      }).
      append($('<iframe>').
        attr('src', widgetUrl + $.param(query)).
        attr('allowTransparency', 'true').
        css({
          border: 'none',
          overflow: 'hidden',
          width: width + 'px',
          height: height + 'px'
        })
      );
  }

  function createVkWidget()
  {
    var mode = 2;
    if (isCompactView()) {
      mode = 1;
      height = 140;
    }

    $box.css({
        overflow: 'hidden',
        width: width + 'px',
        height: height + 'px'
      }).
      append('<div id="vk_groups"></div>');

    var src = location.protocol + '//vk.com/js/api/openapi.';

    $('<script>').attr('src', src + 'js?101').
      appendTo('body');

    var count = 0;

    function showVKGroup()
    {
      if(window.VK && VK.Widgets && VK.Widgets.Group)
      {
        VK.Widgets.Group("vk_groups",
          {mode: mode, width: width, height: height}, 55689929);
        return;
      }

      count++;
      if(count > 20)
        return;

      setTimeout(showVKGroup, 1000);
    }

    setTimeout(showVKGroup, 1000);
  }
});
</script>

